{
  "name": "salvatore",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://thecodersourabh.github.io/TexWeb-dashboard/",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "web": "set VITE_WEB_BUILD=true && vite build && vite preview",
    "prebuild": "npm install --force --save-dev --silent || echo 'Platform dependency handled'",
    "build": "vite build",
    "build:web": "set VITE_WEB_BUILD=true && vite build",
    "build:android": "npm run build && npx cap sync && npx cap open android",
    "build:ci": "npm ci --legacy-peer-deps || npm install --legacy-peer-deps && vite build",
    "build:force": "npm install --force && npm run build",
    "clean:deps": "rm -rf node_modules package-lock.json",
    "sync": "npx cap sync",
    "android": "npx cap open android",
    "lint": "eslint .",
    "preview": "vite preview",
    "run": "vite build && vite preview"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.3.0",
    "@capacitor/android": "^7.4.2",
    "@capacitor/app": "^7.0.1",
    "@capacitor/browser": "^7.0.1",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/geolocation": "^7.1.4",
    "@capacitor/ios": "^7.4.2",
    "@ionic/react": "^8.7.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "terser": "5.43.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.0.0",
    "@rollup/rollup-darwin-x64": "^4.0.0",
    "@rollup/rollup-linux-x64-gnu": "^4.0.0",
    "@rollup/rollup-linux-x64-musl": "^4.0.0",
    "@rollup/rollup-win32-x64-msvc": "^4.0.0"
  }
}
